{
  "version": 2,
  "projects": [
    {
      "name": "draw-project-frontend",
      "rootDirectory": "apps/front-end",
      "buildCommand": "pnpm install --shamefully-hoist && pnpm build",
      "outputDirectory": ".next",
      "framework": "nextjs"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
